  Option_2: "Progenitor Translation Matrix"
  OptionNext_2: Eden_AdminConfig_TranslationControl
  Option_3: "Orbital Refineries"
  OptionNext_3: Eden_AdminConfig_RefineryControl
  Option_4: "Unlock Point Trading"
  OptionNext_4: Eden_AdminConfig_UPControl
  Option_5: "Voidium Harvest Yield"
  OptionNext_5: Eden_AdminConfig_VoidiumControl
  Option_6: "Sortir."
  OptionNext_6: End
}


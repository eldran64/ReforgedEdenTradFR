  Next_2: Eden_ProgCore_Done
}

{ +Dialogue Name: Eden_ProgCore_CoreCount
  #Execute_1: "AddToGlobalVar('Eden_ProgCoreCount', 1); SetSignal('Activated', true); RemoveItem('Token', 1, 6104)"
  Execute_1: "AddToGlobalVar('Eden_ProgCoreCount', 1); SetSignal('Activated', true)"
